{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "build": {
        "env": {
            "NODE_VERSION": "18"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html",
            "status": 200
        }
    ]
}